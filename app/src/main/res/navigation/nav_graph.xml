<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/firstFragment">

    <fragment
        android:id="@+id/firstFragment"
        android:name="com.example.aboutme.FirstFragment"
        android:label="First Fragment">
        <action
            android:id="@+id/action_firstFragment_to_hobbiesFragment"
            app:destination="@id/hobbiesFragment" />
        <action
            android:id="@+id/action_firstFragment_to_contactFragment"
            app:destination="@id/contactFragment" />
    </fragment>

    <fragment
        android:id="@+id/hobbiesFragment"
        android:name="com.example.aboutme.HobbiesFragment"
        android:label="Hobbies Fragment" />

    <fragment
        android:id="@+id/contactFragment"
        android:name="com.example.aboutme.ContactFragment"
        android:label="Contact Fragment" />
</navigation>
